---
// Imports
import "../styles/index.css";
import { Icon } from "astro-icon/components";
import { ViewTransitions } from "astro:transitions";
import DarkMode from "../components/DarkMode.astro";

// Assets images
import { Image } from "astro:assets";
import RyuHero from "../assets/img/goldDragonRed.webp";
import Avatar from "../assets/img/sunset.webp";
import Skills from "../assets/img/mushroom.svg";

const { site } = Astro;
const today = new Date();
const image = new URL("logo-lg.png", site);
const { title, description } = Astro.props;
const props = Astro.props;
---

<html
  lang="en"
  class="scroll-behavior: smooth; h-full motion-safe:scroll-smooth"
>
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="favicon-16x16.png" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
    <meta name="description" content={description} />
    <!-- open graph -->
    <meta property="og:title" content={title} />
    <meta property="og:locale" content="en_US" />
    <meta property="og:type" content="website" />
    <meta property="og:description" content={description} />
    <meta property="og:image" content={image} />
    <meta property="og:url" content={site} />
    <!-- twitter -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content={title} />
    <meta name="twitter:description" content={description} />
    <meta property="twitter:image" content={image} />
    <!-- sitemap -->
    <link rel="sitemap" href="/sitemap-index.xml" />
    <script>
      function setMode() {
        if (
          localStorage.getItem("color-theme") === "dark" ||
          (!("color-theme" in localStorage) &&
            window.matchMedia("(prefers-color-scheme: dark)").matches)
        ) {
          document.documentElement.classList.add("dark");
        } else {
          document.documentElement.classList.remove("dark");
        }
      }
      setMode();
      document.addEventListener("astro:after-swap", setMode);
    </script>
    <ViewTransitions />
  </head>
  <body
    class="bg-gray-50 antialiased transition-colors duration-500 dark:bg-gray-700 text-gray-600 dark:text-gray-50"
  >
    <nav
      class="sticky top-0 z-50 flex justify-between mx-auto font-Futurewave bg-gradient-nav px-1 pr-4 sm:pr-0 sm:px-8 py-2 sm:py-4"
    >
      <a href="/" class="flex justify-center space-x-3 md:justify-start">
        <h2 class="sr-only">OWDDM</h2>
        <div class="flex items-center justify-center">
          <Icon
            name="logomark"
            class="h-14 w-auto rounded text-red-800 hover:text-blue-950"
            aria-label="OWDDM logo"
          />
        </div>
        <span
          class="hidden self-center text-xl text-gradient-brand font-extrabold uppercase tracking-[0.6em] lg:block"
          >owddm | kwddm</span
        >
      </a>
      <div class="flex items-center justify-center">
        <ul class="flex">
          <li>
            <a
              href="#blog"
              class="flex justify-center space-x-3 md:justify-start"
            >
              <h2 class="sr-only">Blog</h2>
              <div class="items-center justify-center block sm:hidden">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="36"
                  height="36"
                  viewBox="0 0 16 16"
                  {...props}
                >
                  <path
                    fill="currentColor"
                    d="M6 0v1.5a8.46 8.46 0 0 1 6.01 2.489a8.472 8.472 0 0 1 2.489 6.01h1.5c0-5.523-4.477-10-10-10z"
                  ></path>
                  <path
                    fill="currentColor"
                    d="M6 3v1.5c1.469 0 2.85.572 3.889 1.611S11.5 8.531 11.5 10H13a7 7 0 0 0-7-7m1.5 3l-1 1L3 8l-3 6.5l.396.396l3.638-3.638a1 1 0 1 1 .707.707l-3.638 3.638l.396.396l6.5-3l1-3.5l1-1l-2.5-2.5z"
                  ></path>
                </svg>
              </div>
              <span
                class="hidden font-extrabold uppercase tracking-[0.6em] sm:block"
                >Blog</span
              >
            </a>
          </li>
          <li>
            <a
              href="#portfolio"
              class="flex justify-center space-x-3 md:justify-start"
            >
              <h2 class="sr-only">Portfolio</h2>
              <div class="items-center justify-center block sm:hidden">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="36"
                  height="36"
                  viewBox="0 0 20 20"
                  {...props}
                >
                  <path
                    fill="currentColor"
                    d="M4 5H.78c-.37 0-.74.32-.69.84l1.56 9.99S3.5 8.47 3.86 6.7c.11-.53.61-.7.98-.7H10s-.7-2.08-.77-2.31C9.11 3.25 8.89 3 8.45 3H5.14c-.36 0-.7.23-.8.64C4.25 4.04 4 5 4 5m4.88 0h-4s.42-1 .87-1h2.13c.48 0 1 1 1 1M2.67 16.25c-.31.47-.76.75-1.26.75h15.73c.54 0 .92-.31 1.03-.83c.44-2.19 1.68-8.44 1.68-8.44c.07-.5-.3-.73-.62-.73H16V5.53c0-.16-.26-.53-.66-.53h-3.76c-.52 0-.87.58-.87.58L10 7H5.59c-.32 0-.63.19-.69.5c0 0-1.59 6.7-1.72 7.33c-.07.37-.22.99-.51 1.42M15.38 7H11s.58-1 1.13-1h2.29c.71 0 .96 1 .96 1"
                  ></path>
                </svg>
              </div>
              <span
                class="hidden font-extrabold uppercase tracking-[0.6em] sm:block"
                >Portfolio</span
              >
            </a>
          </li>
        </ul>
        <!-- darkmode icon comes here -->
        <DarkMode />
      </div>
    </nav>
    <section class="h-full">
      <Image
        src={RyuHero}
        alt="Call to action image"
        loading="eager"
        class="absolute object-cover h-screen"
      />
      <div
        class="relative grid h-screen place-items-center bg-red-800/80 dark:bg-gray-800/90 p-6"
      >
        <div
          class="flex flex-col-reverse lg:flex-row items-center gap-12 px-0 sm:px-6"
        >
          <div class="text-grey-950">
            <div class="text-center">
              <h1 class="font-Futurewave text-xs sm:text-xl">
                an <span
                  class="text-sm sm:text-4xl font-extrabold font-Futurewave text-gradient-hero"
                  >OWDDM
                </span>Workshop<br />with<span
                  class="text-sm sm:text-2xl font-extrabold font-Futurewave"
                >
                  Romano Moesa</span
                >
              </h1>
              <h2
                class="text-gradient-hero text-sm sm:text-xl font-semibold uppercase tracking-widest sm:text-right"
              >
                TBA March or April :2024
              </h2>
              <p
                class="mt-6 max-w-xl mx-auto leading-7 text-gradient-hero font-Futurewave text-xs sm:text-base"
